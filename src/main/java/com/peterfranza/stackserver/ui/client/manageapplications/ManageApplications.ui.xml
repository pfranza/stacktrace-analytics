<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
		<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:VerticalPanel>
				<g:ListBox ui:field="listBox" visibleItemCount="20"></g:ListBox>
				<g:Button ui:field="remove">Delete</g:Button>
			</g:VerticalPanel>
			<g:VerticalPanel>
				<g:TextBox ui:field="applicationName"></g:TextBox>
				<g:TextBox ui:field="applicationDescription"></g:TextBox>
				<g:Button ui:field="add">Add</g:Button>
				<g:HTMLPanel>
				  <hr/>
				  <g:Label ui:field="apiKey"/>
				  <g:Label ui:field="token"/>
				  <g:Label ui:field="consumer"/>  
				</g:HTMLPanel>    
			</g:VerticalPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 